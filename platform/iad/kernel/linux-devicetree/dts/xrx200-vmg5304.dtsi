#include <dt-bindings/gpio/gpio.h>
#include "xrx200.dtsi"

/ {
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;

		pinctrl-names = "default";
		pinctrl-0 = <&pins_gpio_keys_default>;

		reset {
			label = "reset-btn";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			linux,code = <0x100>;	/* BTN_0 */
		};
		wlan {
			label = "wlan-btn";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
			linux,code = <0x101>;	/* BTN_1 */
		};
		wps {
			label = "wps-btn";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
			linux,code = <0x102>;	/* BTN_2 */
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		pinctrl-names = "default";
		pinctrl-0 = <&pins_gpio_leds_default>;

		wps_amber {
			label = "amber:wps";
			gpios = <&gpio 9 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		wlan_green {
			label = "green:wlan";
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		web_green {
			label = "green:web";
			gpios = <&gpio 12 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		power_red {
			label = "red:power";
			gpios = <&gpio 15 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		power_green {
			label = "green:power";
			gpios = <&gpio 33 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		phone_green {
			label = "green:phone";
			gpios = <&stp 7 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		info_green {
			label = "green:info";
			gpios = <&stp 6 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		phone_red {
			label = "red:phone";
			gpios = <&stp 4 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		info_red {
			label = "red:info";
			gpios = <&stp 3 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		web_red {
			label = "red:web";
			gpios = <&stp 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		dsl_green {
			label = "green:dsl-led1";
			gpios = <&stp 0 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	sph_platform {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "sphairon,platform";
	};
};

&mps {
	pinctrl-names = "default", "master", "slave";
	pinctrl-0 = <&pins_tdm_default &pins_ssio_default>;
	pinctrl-1 = <&pins_tdm_master>;
	pinctrl-2 = <&pins_tdm_slave>;

	lantiq,slic-reset = <&gpio 31 GPIO_ACTIVE_HIGH>;
};

&localbus {
	nand@1 {
		compatible = "gen_nand", "lantiq,nand-xway";
		reg = <1 0x0 0x2000000>;
		linux,mtd-name = "nand-xway";
		bbt-use-flash;
		bank-width = <2>;
		lantiq,cs = <1>;
		pinctrl-names = "default";
		pinctrl-0 = <&pins_nand_default>;
	};
};

&spi {
	pinctrl-names = "default";
	pinctrl-0 = <&pins_spi_default>;

	status = "ok";

	m25p80@4 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spansion,m25p80";
		reg = <4>;
		spi-max-frequency = <25000000>;
	};
};

&gpio {
	pinctrl-names = "default";
	pinctrl-0 = <&pins_default>;

	pins_default: pins_default {
	};

	pins_pcie_default: pins_pcie_default {
		pcie_reset {
			lantiq,pins = "io38";
			lantiq,output = <1>;
			lantiq,open-drain = <1>;
		};
		pcie_clkreq {
			lantiq,pins = "io0";
			lantiq,output = <0>;
		};
	};

	pins_gpio_keys_default: pins_gpio_keys_default {
		gpio_in {
			lantiq,pins = "io1", "io2", "io7";
			lantiq,output = <1>;
		};
	};

	pins_gpio_leds_default: pins_gpio_leds_default {
		gpio_high_active {
			lantiq,pins = "io9", "io11", "io12",
					"io15", "io33";
			lantiq,output = <1>;
		};
	};

	pins_nand_default: pins_nand_default {
		nand_out {
			lantiq,groups = "ebu cs1", "nand cle",
					"nand ale", "nand rd";
			lantiq,function = "ebu";
			lantiq,output = <1>;
		};
		nand_in {
			lantiq,groups = "nand rdy";
			lantiq,function = "ebu";
			lantiq,output = <0>;
		};
	};

	pins_spi_default: pins_spi_default {
		spi_in {
			lantiq,groups = "spi_din";
			lantiq,function = "spi";
			lantiq,output = <0>;
		};
		spi_out {
			lantiq,groups = "spi_dout", "spi_clk",
					"spi_cs4";
			lantiq,function = "spi";
			lantiq,output = <1>;
			lantiq,open-drain = <0>;
		};
	};

	pins_stp_default: pins_stp_default {
		stp {
			lantiq,groups = "stp";
			lantiq,function = "stp";
			lantiq,output = <1>;
		};
	};

	pins_tdm_default: pins_tdm_default {
		tdm_clock {
			lantiq,groups = "tdm fsc", "tdm dcl";
			lantiq,function = "gpio";
			lantiq,pull = <2>;
			lantiq,open-drain = <0>;
			lantiq,output = <0>;
		};
		tdm_data {
			lantiq,groups = "tdm di", "tdm do";
			lantiq,function = "gpio";
			lantiq,pull = <2>;
			lantiq,open-drain = <0>;
			lantiq,output = <0>;
		};
	};

	pins_tdm_master: pins_tdm_master {
		tdm_clock {
			lantiq,groups = "tdm fsc", "tdm dcl";
			lantiq,function = "tdm";
			lantiq,pull = <0>;
			lantiq,open-drain = <0>;
			lantiq,output = <1>;
		};
		tdm_di {
			lantiq,groups = "tdm di";
			lantiq,function = "tdm";
			lantiq,pull = <2>;
			lantiq,open-drain = <0>;
			lantiq,output = <0>;
		};
		tdm_do {
			lantiq,groups = "tdm do";
			lantiq,function = "tdm";
			lantiq,pull = <0>;
			lantiq,open-drain = <0>;
			lantiq,output = <1>;
		};
	};

	pins_tdm_slave: pins_tdm_slave {
		tdm_clock {
			lantiq,groups = "tdm fsc", "tdm dcl";
			lantiq,function = "tdm";
			lantiq,pull = <2>;
			lantiq,open-drain = <0>;
			lantiq,output = <0>;
		};
		tdm_di {
			lantiq,groups = "tdm di";
			lantiq,function = "tdm";
			lantiq,pull = <2>;
			lantiq,open-drain = <0>;
			lantiq,output = <0>;
		};
		tdm_do {
			lantiq,groups = "tdm do";
			lantiq,function = "tdm";
			lantiq,pull = <0>;
			lantiq,open-drain = <0>;
			lantiq,output = <1>;
		};
	};

	pins_ssio_default: pins_ssio_default {
		ssio_out {
			lantiq,groups = "ssio tx";
			lantiq,function = "ssio";
			lantiq,output = <1>;
		};
		ssio_in {
			lantiq,groups = "ssio rx", "ssio clk";
			lantiq,function = "ssio";
			lantiq,output = <0>;
		};
		clk {
			lantiq,groups = "clkout0";
			lantiq,function = "cgu";
			lantiq,output = <1>;
		};
		slic_reset {
			lantiq,pins = "io31";
			lantiq,pull = <0>;
			lantiq,open-drain = <0>;
			lantiq,output = <1>;
			lantiq,drive = <1>;
		};
	};
};

#if !defined(IS_UGW_61)
&gphy0 {
	lantiq,phy-mode = "11g";
	status = "ok";
};

&gphy1 {
	lantiq,phy-mode = "11g";
	status = "ok";
};
#endif

&eth {
#if defined(IS_UGW_61)
	gphy@0 {
		lantiq,phy-mode = "11g";
		status = "ok";
	};

	gphy@1 {
		lantiq,phy-mode = "11g";
		status = "ok";
	};
#endif

	mdio@0 {
		phy0: ethernet-phy@11 {
			compatible = "lantiq,pef7071", "ethernet-phy-ieee802.3-c22";
			/*interrupt-parent = <&icu0>;
			interrupts = <122>;*/
			reg = <0x11>;
			lantiq,led0 = <0x7003>;
			lantiq,led1 = <0x0>;
			lantiq,led2 = <0x0>;
		};

		phy1: ethernet-phy@13 {
			compatible = "lantiq,pef7071", "ethernet-phy-ieee802.3-c22";
			/*interrupt-parent = <&icu0>;
			interrupts = <121>;*/
			reg = <0x13>;
			lantiq,led0 = <0x7003>;
			lantiq,led1 = <0x0>;
			lantiq,led2 = <0x0>;
		};
	};

	ethernet@2 {
		phy-handle = <&phy0>;
		phy-mode = "gmii";
		status = "ok";
		lantiq,port-name = "sw-p2-lan2";
		lantiq,wanoe;
	};

	ethernet@4 {
		phy-handle = <&phy1>;
		phy-mode = "gmii";
		status = "ok";
		lantiq,port-name = "sw-p4-lan1";
	};
};

&stp {
	pinctrl-names = "default";
	pinctrl-0 = <&pins_stp_default>;

	status = "ok";

	lantiq,shadow = <0x0>;
	lantiq,groups = <0x1>;
	lantiq,speed = <0x8>;
	lantiq,phy1 = <0x1>;
	lantiq,phy2 = <0x1>;
};

&pcie0 {
	pinctrl-names = "default";
	pinctrl-0 = <&pins_pcie_default>;

	status = "ok";
	lantiq,reset-gpio = <&gpio 38 GPIO_ACTIVE_LOW>;
	lantiq,rst-interval = <200>;
	lantiq,inbound-swap = <1>;
	lantiq,outbound-swap = <1>;
	lantiq,phy-mode = <1>;
};
